      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "express": "^4.21.0",
      "express-validator": "^7.2.0",
      "ioredis": "^5.4.1",
      "jsonwebtoken": "^9.0.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20",
      "typescript": "^5.6.2"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
